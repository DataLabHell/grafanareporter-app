{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/main/docs/sources/developers/plugins/plugin.schema.json",
  "type": "app",
  "name": "Grafana-Reporter",
  "id": "datalabhell-grafanareporter-app",
  "info": {
    "keywords": [
      "app"
    ],
    "description": "Render Grafana dashboards to PDF directly in the browser using the grafana-image-renderer, implemented as a pure front-end plugin.",
    "author": {
      "name": "Daniel Baldauf"
    },
    "links": [
      {
        "name": "Source Code",
        "url": "https://github.com/datalabhell/grafanareporter-app"
      },
      {
        "name": "License",
        "url": "https://github.com/datalabhell/grafanareporter-app/blob/main/LICENSE"
      }
    ],
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "screenshots": [
      {
        "name": "Configuration",
        "path": "img/configuration.png"
      },
      {
        "name": "Reporter App",
        "path": "img/reporter.png"
      }
    ],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "includes": [
    {
      "type": "page",
      "name": "Grafana Reporter",
      "path": "/a/%PLUGIN_ID%",
      "addToNav": true
    }
  ],
  "dependencies": {
    "grafanaDependency": ">=11.2.10",
    "plugins": []
  }
}
